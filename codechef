impot java.util.*;
public class Armstrong{
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
int a,a1,r=0,s=0;
System.out.println("Enter a number");
a=sc.nextInt();
a1=a;
while(a!=0){
r=a%10;
s+=r*r*r;
a=a/10;
}
if(s==a1)
System.out.println("Armstrong Number");
else
System.out.println("Not an Armstrong number");
}
}
